{
	"name": "ztaro",
	"version": "1.0.0",
	"description": "taro combine zoro tool",
	"main": "index.js",
	"keywords": [
		"taro",
		"zoro",
		"redux",
		"alioss",
		"weapp"
	],
	"scripts": {
		"build:weapp": "cross-env-shell BUILD_ENV=prod \"taro build --type weapp && npm run upload:oss\"",
		"build:weapp-dev": "cross-env-shell BUILD_ENV=dev \"taro build --type weapp && npm run upload:oss\"",
		"build:h5": "cross-env BUILD_ENV=prod taro build --type h5",
		"build:h5-dev": "cross-env BUILD_ENV=dev taro build --type h5",
		"dev:weapp": "concurrently \"cross-env BUILD_ENV=dev taro build --type weapp -- --watch\" \"cross-env BUILD_ENV=dev gulp watch\"",
		"mock:weapp": "concurrently \"npm run server\" \"cross-env ENABLE_MOCK=true BUILD_ENV=dev taro build --type weapp -- --watch\" \"cross-env BUILD_ENV=dev gulp watch\"",
		"dev:h5": "npm run build:h5 -- --watch",
		"mock:h5": "cross-env ENABLE_MOCK=true npm run build:h5-dev -- --watch",
		"server": "node-dev ./server/bin/www",
		"upload:oss": "gulp upload:oss",
		"lint": "eslint ./src",
		"validate": "npm ls"
	},
	"author": "FaureWu <502556093@qq.com>",
	"license": "MIT",
	"dependencies": {
		"@opcjs/zoro": "^2.3.7",
		"@opcjs/zoro-plugin": "^2.2.0",
		"@tarojs/async-await": "1.3.0-beta.6",
		"@tarojs/components": "1.3.0-beta.6",
		"@tarojs/redux": "1.3.0-beta.6",
		"@tarojs/redux-h5": "1.3.0-beta.6",
		"@tarojs/router": "1.3.0-beta.6",
		"@tarojs/taro": "1.3.0-beta.6",
		"@tarojs/taro-h5": "1.3.0-beta.6",
		"@tarojs/taro-weapp": "1.3.0-beta.6",
		"classnames": "^2.2.6",
		"nervjs": "^1.3.13",
		"reselect": "^4.0.0"
	},
	"devDependencies": {
		"@tarojs/cli": "1.3.0-beta.6",
		"@tarojs/plugin-babel": "1.3.0-beta.6",
		"@tarojs/plugin-csso": "1.3.0-beta.6",
		"@tarojs/plugin-sass": "1.3.0-beta.6",
		"@tarojs/plugin-uglifyjs": "1.3.0-beta.6",
		"@tarojs/webpack-runner": "1.3.0-beta.6",
		"@types/react": "^16.8.19",
		"@types/webpack-env": "^1.13.9",
		"alioss-upload-loader": "^1.0.2",
		"babel-eslint": "^10.0.1",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-plugin-transform-jsx-stylesheet": "^0.6.5",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"concurrently": "^4.0.1",
		"cookie-parser": "^1.4.4",
		"cross-env": "^5.2.0",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^4.3.0",
		"eslint-config-taro": "1.3.0-beta.6",
		"eslint-plugin-import": "^2.17.3",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-react": "^7.13.0",
		"eslint-plugin-taro": "1.3.0-beta.6",
		"express": "^4.17.1",
		"faker": "^4.1.0",
		"gulp": "^4.0.2",
		"gulp-alioss-upload": "^1.0.3",
		"morgan": "^1.9.1",
		"node-dev": "^4.0.0",
		"precommit-hook": "^3.0.0",
		"prettier": "^1.17.1"
	},
	"pre-commit": [
		"lint"
	],
	"repository": "git@github.com:FaureWu/ztaro.git"
}
